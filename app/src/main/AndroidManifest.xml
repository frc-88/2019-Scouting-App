<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.robotics.tj2.scout88">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".extensions.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.PreMatchPreviewActivity"></activity>
        <activity
            android:name=".activities.MatchPreviewActivity"
            android:label="@string/title_activity_match_preview"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activities.DataSyncActivity" />
        <activity
            android:name=".activities.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name=".activities.TeamTableActivity" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>